{
  "name": "ng-sandbox",
  "version": "0.0.0",
  "scripts": {
    "start": "npm run start:sandbox",
    "start:sandbox": "ng serve sandbox",
    "build": "npm run build:sandbox-lib && npm run build:sandbox",
    "build:sandbox": "ng build sandbox",
    "build:sandbox-lib": "ng build sandbox-lib",
    "watch:sandbox-lib": "ng build --watch --configuration development sandbox-lib",
    "test": "npm run test:sandbox-lib && npm run test:sandbox",
    "test:sandbox": "jest --config ./projects/sandbox/jest.config.ts",
    "test:sandbox-lib": "jest --config ./projects/sandbox-lib/jest.config.ts",
    "lint": "ng lint",
    "format": "prettier --write ."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.0.0",
    "@angular/cdk": "^16.0.3",
    "@angular/common": "^16.0.0",
    "@angular/compiler": "^16.0.0",
    "@angular/core": "^16.0.0",
    "@angular/forms": "^16.0.0",
    "@angular/material": "^16.0.3",
    "@angular/platform-browser": "^16.0.0",
    "@angular/platform-browser-dynamic": "^16.0.0",
    "@angular/router": "^16.0.0",
    "@ngrx/component": "^16.0.1",
    "@ngrx/component-store": "^16.0.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.0.4",
    "@angular-eslint/builder": "16.0.3",
    "@angular-eslint/eslint-plugin": "16.0.3",
    "@angular-eslint/eslint-plugin-template": "16.0.3",
    "@angular-eslint/schematics": "16.0.3",
    "@angular-eslint/template-parser": "16.0.3",
    "@angular/cli": "~16.0.4",
    "@angular/compiler-cli": "^16.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "5.59.7",
    "@typescript-eslint/parser": "5.59.7",
    "eslint": "^8.40.0",
    "jest": "^29.5.0",
    "jest-preset-angular": "^13.1.1",
    "ng-mocks": "^14.10.1",
    "ng-packagr": "^16.0.0",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.3",
    "typescript": "~5.0.2"
  }
}